#ifdef VERDLL
			VALUE "OriginalFilename", "EVTSYS.DLL"
#else
			VALUE "OriginalFilename", "EVTSYS.EXE"
#endif
			VALUE "ProductName", "Eventlog to Syslog Utility"
			VALUE "ProductVersion", VERSION
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x0409, 0x04b0
	END
